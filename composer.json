    "require-dev": {
        "consolidation/robo": "^5.0",
        "maddhatter/markdown-table": "^1.2",
        "nuvoleweb/robo-config": "^3.0",
        "pamald/robo-pamald-composer": "1.x-dev",
        "pamald/robo-pamald-npm": "1.x-dev",
        "pamald/robo-pamald-yarn": "1.x-dev",
        "sweetchuck/git-hooks": "2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Pamald\\ExampleProject\\": "src/"
        }
    }
}
